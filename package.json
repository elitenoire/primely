{
  "name": "primely",
  "version": "1.0.0",
  "description": "Keep records of your students",
  "main": "server/server.js",
  "engines": {
    "node": "8.4.0",
    "npm": "5.5.1"
  },
  "scripts": {
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elitenoire/primely.git"
  },
  "keywords": [
    "sis",
    "admin",
    "dashboard",
    "school"
  ],
  "author": "Evarista Igwe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elitenoire/primely/issues"
  },
  "homepage": "https://github.com/elitenoire/primely#readme",
  "dependencies": {
    "ajv": "^5.2.4",
    "ajv-keywords": "^2.1.0",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.0",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.0",
    "mongoose": "^4.12.5",
    "redux-form-with-ajv": "^0.3.0"
  }
}
